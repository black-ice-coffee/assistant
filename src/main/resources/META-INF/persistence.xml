<persistence xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			 xsi:schemaLocation="http://java.sun.com/xml/ns/persistence
    http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
			 version="2.0">
	<persistence-unit name="assistant">
		<class>com.assistant.entity.NewsEntity</class>
		<class>com.assistant.entity.RssEntity</class>
        <class>com.assistant.entity.NoteEntity</class>
        <properties>
			<property name="hibernate.connection.url" value="jdbc:postgresql://192.168.99.100:32768/assistant"/>
			<property name="hibernate.connection.driver_class" value="org.postgresql.Driver"/>
			<property name="hibernate.connection.username"/>
			<property name="hibernate.connection.password"/>
		</properties>
	</persistence-unit>
</persistence>